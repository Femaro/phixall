{
  "expo": {
    "name": "Phixall",
    "slug": "phixall-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phixall.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.phixall.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAEoieAgpx2ExNauC1IoauAZV-UsXh1MTg"
        }
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Phixall to use your location for job tracking."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Phixall to access your photos for job attachments."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2563eb"
        }
      ]
    ],
    "scheme": "phixall",
    "extra": {
      "firebaseApiKey": "AIzaSyAEoieAgpx2ExNauC1IoauAZV-UsXh1MTg",
      "firebaseAuthDomain": "phixall-4c0a2.firebaseapp.com",
      "firebaseProjectId": "phixall-4c0a2",
      "firebaseStorageBucket": "phixall-4c0a2.firebasestorage.app",
      "firebaseMessagingSenderId": "116906563573",
      "firebaseAppId": "1:116906563573:web:4ac86209e77ed2c98432bc",
      "router": {},
      "eas": {
        "projectId": "afc103a4-ca3f-4fa0-ae62-8ddd5a041b6e"
      }
    },
    "owner": "medullaz"
  }
}
